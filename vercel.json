{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.ts" },
    { "source": "/oauth/(.*)", "destination": "/api/index.ts" },
    { "source": "/graphql", "destination": "/api/index.ts" },
    { "source": "/docs/(.*)", "destination": "/api/index.ts" },
    { "source": "/public-profile/:slug", "destination": "/api/index.ts" }
  ]
}
